{
  "name": "mia-vn-google-integration",
  "version": "1.0.0",
  "description": "MIA.vn Google Integration Platform - Comprehensive automation and data management system",
  "private": true,
  "homepage": ".",
  "author": "MIA.vn Team",
  "license": "MIT",
  "keywords": [
    "react",
    "google-sheets",
    "google-drive",
    "automation",
    "mia-vn"
  ],
  "dependencies": {
    "@ant-design/icons": "^6.0.2",
    "@testing-library/dom": "^10.4.1",
    "@types/node": "^24.5.2",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "antd": "^5.27.4",
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.18",
    "express": "^5.1.0",
    "formidable": "^3.5.4",
    "google-auth-library": "^10.3.0",
    "googleapis": "^160.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.28.0",
    "react-scripts": "5.0.1",
    "recharts": "^3.2.1",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prod": "GENERATE_SOURCEMAP=false CI=false react-scripts build",
    "test": "react-scripts test --coverage --watchAll=false --testTimeout=10000",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "test:google": "node scripts/testGoogleConnection.js",
    "setup": "node scripts/setup.js",
    "deploy": "node scripts/deploy.js",
    "health-check": "node scripts/health-check.js",
    "test:integration": "npm run test:google && npm run health-check",
    "lint": "eslint src/ --ext .js,.jsx --fix --max-warnings=0",
    "lint:check": "eslint src/ --ext .js,.jsx --max-warnings=0",
    "format": "prettier --write src/ --ignore-path .gitignore",
    "format:check": "prettier --check src/ --ignore-path .gitignore",
    "clean": "rimraf build node_modules package-lock.json",
    "clean:build": "rimraf build",
    "dev": "concurrently \"npm start\" \"node server.js\"",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "serve": "serve -s build -l 3000",
    "preview": "npm run build:prod && npm run serve"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn",
      "prefer-const": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "concurrently": "^8.0.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.36.0",
    "http-proxy-middleware": "^3.0.5",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.0",
    "serve": "^14.0.0",
    "typescript": "^5.9.2",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^4.15.1",
    "svgo": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mia-vn/google-integration.git"
  },
  "bugs": {
    "url": "https://github.com/mia-vn/google-integration/issues"
  }
}
